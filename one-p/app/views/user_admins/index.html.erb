<div class="main_page">
   <% if !logged_in? %>
      <p><%= link_to 'Login', new_admin_session_path %>
      <%= link_to 'Signup', new_user_admin_path %>
    <% else %>
      <%= link_to "Log out", logout_path %></p>
    <% end %>
  </div></p>

<div class="main_page">
   <% if logged_in? %>
      <p><% @contacts.each do |contact| %>  
        <%= contact.name %>  
        <%= contact.email %>  
        <%= contact.number %>
          <%= contact.message %>
        </p></br>
      <% end %>
    <% else %>
      'please login to view contacts.'</p>
    <% end %>
  </div></p>